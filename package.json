{
  "name": "live-js-sdk-demo-normal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run lint && vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint --ext .js,vue . --ignore-pattern .gitignore",
    "lint:fix": "eslint --fix --ext .js,vue . --ignore-pattern .gitignore"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@polyv/eslint-config": "^0.2.5",
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-router": "~4.5.8",
    "@vue/cli-plugin-vuex": "~4.5.8",
    "@vue/cli-service": "~4.5.8",
    "eslint": "^8.17.0",
    "yorkie": "2.0.0",
    "lint-staged": "^9.5.0",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "npm run lint",
      "git add"
    ]
  }
}
